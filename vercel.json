{
    "version": 2,
    "builds": [
        {
            "src": "project_name/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "project_name/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "project_name.settings"
    }
}