{
    "version": 2,
    "build": [
        {
            "src": "MyCalculatorProject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "MyCalculatorProject/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "MyCalculatorProject.settings"
    }
}